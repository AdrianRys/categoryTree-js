/*video player + ie6/7 fixes*/

.lightOff {
position:fixed;
width:100%;
height:100%;
z-index:80;
top:0;
left:-9999px;
background:#000;
background:rgba(0,0,0,.9) }

#mediaBox.lightOff {
z-index:1000 }

.full .lightOff,
.left .lightOff,
.right .lightOff {
left:0;
position:absolute }

.show {
left:0;
display:inline }

.center {
display:table;
overflow:hidden;
margin:0 auto }

:first-child+html .center {
position:relative }

* html .center {
position:relative }

.mediaHolder {
display:table-cell;
vertical-align:middle }

*:first-child+html .mediaHolder {
position:absolute;
top:50% }

* html .mediaHolder {
position:absolute;
top:50% }

*:first-child+html .media {
position:relative;
top:-50% }

* html .media {
position:relative;
top:-50% }

.center {
height:100%;
width:85% }

.media {
width:100%;
position:relative }

.center {
overflow:visible }

span.close {
float:left;
background:#000;
