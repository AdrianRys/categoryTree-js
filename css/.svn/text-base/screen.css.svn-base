/*images*/

.slider,
.left,
.right,
.full {
position:relative }

img {
width:100%;
display:block }

.right {
margin:5px 0 25px 25px;
clear:right;
float:right;
width:50% }

.left {
margin:7px 25px 25px 0;
float:left;
clear:left;
width:50% }

.full {
width:100%;
