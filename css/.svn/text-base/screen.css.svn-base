

/*don't edit this*/


/*print css*/

@import url(../custom/vyre/custom.css);


/*reset*/

* {
padding:0;
margin:0;
border:0;
outline:0;
offset:0;
list-style:none }

body,
input,
textarea,
select,
th,
td {
-webkit-backface-visibility:hidden;
-webkit-text-size-adjust:none;
text-shadow:rgba(0,0,0,.01) 0 0 1px;
-webkit-font-smoothing:antialiased;
text-rendering:optimizeLegibility;
color:#333;
font-family:'Helvetica Neue', Arial, sans-serif;
font-size:13px;
line-height:20px }

body,
html {
background:#fff;
height:100%;
text-shadow:none }

a {
text-decoration:none;
color:#2382E1 }

a:hover {
text-decoration:underline }

.hide {
display:none }

.clear {
clear:both }

.bottomPadding {
height:50px;
clear:both }

a:active {
position:relative;
top:1px }

.active a:active {
top:0 }

.active a:hover {
cursor:default;
text-decoration:none }

html {
min-height:100% }


/*tables*/

table,
th,
td {
border-collapse:collapse;
border-spacing:0 }

table {
width:100%;
margin:0 0 25px;
background:#fff;
float:left;
border-top:solid 1px #e0e0e0;
border-left:solid 1px #e0e0e0;
border-bottom:solid 1px #e0e0e0;
border-right:solid 1px #e0e0e0 }

table th {
text-align:left;
border-top:solid 1px #e0e0e0;
border-bottom:solid 1px #e0e0e0;
padding:5px 10px 5px 10px;
background:#efefef }

table tr {
vertical-align:top }

table tbody tr.even {
background:#f4f7fb }

table td {
padding:6px 10px 6px 10px; }


/*table extras*/

table a.remove {
text-indent:-9999px;
display:block;
background:url(../images/removeBlue.png) no-repeat left center }

table a.remove:hover {
background:url(../images/removeRed.png) no-repeat left center }


/*forms*/

form {
float:left;
width:300px }

button::-moz-focus-inner,
input::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
border:none }

fieldset {
padding:25px;
margin:0 0 25px;
border:solid 1px #c8c8c8 }

legend {
font-size:15px;
font-weight:500;
padding:5px 10px 5px 10px }

label {
font-size:13px;
margin:0 0 2px;
display:block }

label.checkbox {
clear:both;
display:inline }

input.text,
input.password,
textarea {
border:outset 1px #c8c8c8;
font-size:15px;
padding:5px;
width:288px;
margin:0 0 10px }

textarea {
height:58px;
line-height:19px }

input.text:focus,
input.password:focus,
textarea:focus {
border:inset 1px #c8c8c8;
background:#fefeee }

select {
border:1px solid;
cursor:pointer }

input.file:focus {
background:#fefeee; }

input.checkbox,
input.radio {
clear:both;
margin:0 6px 0 0 }

input.checkbox:focus,
input.radio:focus {
margin:0 10px 0 0 }

input.file {
margin:0 0 20px;
border:solid 1px #c8c8c8;
padding:5px 5px 5px 5px;
width:288px;
cursor:pointer;
margin:0 0 10px;
font-size:13px }

input.button,
input.submit,
input.save {
display:block;
clear:both;
margin:15px 0 20px;
padding:2px 15px 2px 15px;
text-shadow:1px 1px 1px #fff;
font-size:13px;
font-weight:500;
cursor:pointer;
border:solid 1px #bbb;
background:#f3f3f3 }

input.button:hover,
input.submit:hover,
input.button:focus,
input.submit:focus,
input.save:hover,
input.save:focus {
color:#2382E1;
background:#e1e1e1 }

input.button:active,
input.submit:active,
input.save:active {
padding:3px 15px 1px 15px }

hr {
clear:both;
height:1px;
background:#d9d9d9;
margin:20px 0 20px 0 }

legend {
margin:0 0 20px 0\9 }


/*typography*/

h1 {
margin-bottom:15px;
color:#181818;
font-size:26px;
line-height:35px;
padding:15px 250px 5px 0 }

h2 {
color:#181818;
font-size:18px;
line-height:40px;
padding:0 0 5px }

h3 {
font-size:16px;
font-weight:700;
line-height:40px;
padding:0 0 5px }

h4 {
clear:both;
font-size:14px;
font-weight:500;
line-height:24px;
padding:10px 0 5px }

h5 {
clear:both;
font-size:14px;
font-weight:500;
line-height:24px;
padding:0 0 5px }

h6 {
clear:both;
font-size:12px;
line-height:24px;
padding:0 0 5px }

p {
color:#505050;
font-size:15px;
line-height:25px;
padding:0 0 15px }

strong {
font-weight:700 }

em {
font-style:italic }

.fullFluid .articleBody ul,
.fullFluid .articleBody ol {
text-indent:10px;
font-size:15px;
line-height:25px;
padding:0 0 10px 10px }

.fullFluid .articleBody ul li {
list-style:disc inside }

.fullFluid .articleBody ol li {
list-style:decimal inside }


/*layout*/

div.fullFluid {
min-width:940px;
max-width:1260px;
clear:both;
width:100%;
position:relative }

div.rightFluid {
width:auto;
clear:both;
position:relative;
padding:35px 50px 0 300px }

div.leftFixed {
