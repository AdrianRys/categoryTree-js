border-right:1px solid rgba(0,0,0,0) /*opera transparent bg fix*/ }

div.header form.search input.submit {
height:37px;
width:37px }

div.header form.search input.submit:active {
background:url(../images/searchIcon.png) center 12px no-repeat #efefef }

form.search {
position:relative;
width:300px;
float:left }

div.header form.search {
position:absolute;
right:50px;
top:88px }

form.search input.text {
height:20px;
line-height:23px;
padding:8px 50px 9px 9px;
display:inline;
float:left;
background:#fff;
width:238px;
border:solid 1px #e0e0e0;
font-size:15px;
font-weight:400 }

form.search input.text:focus {
background:#fefeee }

form.search label {
color:#999;
width:240px;
font-size:15px;
line-height:23px;
height:20px;
font-weight:400;
padding:7px 50px 9px 9px;
