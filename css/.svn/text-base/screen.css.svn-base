position:absolute;
top:0 }

.fullFluid form.search label {
padding:8px 50px 9px 9px;
left:1px }

form.search input.submit {
margin:0;
position:absolute;
z-index:1;
cursor:pointer;
top:0;
right:0;
height:39px;
width:39px;
border:solid 1px #e0e0e0;
background:#fff;
text-indent:-9999px;
background:url(../images/searchIcon.png) center no-repeat #fff }

form.search input.submit:hover {
