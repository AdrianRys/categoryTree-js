.slider {
float:left;
margin-bottom:25px }

.slider.right {
float:right }

.controls {
position:fixed;
top:0;
opacity:1;
-o-transition:opacity .25s linear;
-moz-transition:opacity .25s linear;
-webkit-transition:opacity .25s linear;
transition:opacity .25s linear;
z-index:1;
margin:0px 20%;
width:60% }

.left .controls,
.right .controls,
.full .controls,
#assetPreview .controls {
