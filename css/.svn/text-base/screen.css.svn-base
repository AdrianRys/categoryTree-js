width:250px;
position:absolute;
clear:both;
top:0;
left:0;
z-index:1 }

div.leftFluid {
width:auto;
clear:both;
position:relative;
padding:0 300px 0 50px }

div.rightFixed {
width:250px;
position:absolute;
clear:both;
top:0;
right:0;
z-index:1 }


/*header*/

div.headerArea {
float:left;
position:relative;
width:100%;
min-width:940px;
max-width:1260px }

div.headerArea {
height:140px }

div.header {
width:100%;
min-width:940px;
background:#303030 }

div.header ol {
display:inline;
float:left;
margin:90px 0 0 38px }

div.header ol li {
display:inline;
height:50px;
width:auto;
float:left }

div.header ol li a {
float:left;
font-size:16px;
font-weight:500;
line-height:20px;
display:block;
padding:13px 12px 17px 12px;
