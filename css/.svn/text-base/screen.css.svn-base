form.search label.fade {
-o-transition:opacity .2s linear;
-moz-transition:opacity .2s linear;
-webkit-transition:opacity .2s linear;
transition:opacity .2s linear;
opacity:.5 }

.userActions {
color:#fff;
position:absolute;
top:8px;
right:101px }

a.options {
font-size:13px;
font-weight:500;
color:#fff;
padding:0 14px 0;
margin:0 5px 0 0;
background:url(../images/dropWhite.png) no-repeat right 6px }

.userActions .dialog {
top:27px;
right:-10px }

a.logoutButton {
position:absolute;
top:8px;
right:50px;
font-size:13px;
font-weight:500;
color:#fff } 

div.subNav {
width:auto;
clear:both;
height:44px;
border-bottom:solid 1px #e0e0e0;
background:#fff }

div.subNav.empty {
height:14px }

div.subNav ul {
float:left;
position:relative;
width:100%;
min-width:940px;
max-width:1260px }

div.subNav ul li {
width:auto;
float:left }

div.subNav ul a {
float:left;
font-size:15px;
font-weight:500;
line-height:18px;
display:block;
padding:13px 10px 13px 10px;
position:relative;
left:40px }

div.subNav ul li.active a {
color:#303030 }


/*article navigation*/

ul.articleNav {
width:100%;
float:left }

ul.articleNav li {
width:100%;
float:left;
border-right:solid 1px #e0e0e0;
border-bottom:solid 1px #e0e0e0 }

ul.articleNav li a {
